{
  "name": "ESP32-S3 DevKitC-1 N16R8V (16MB Flash, 8MB PSRAM)",
  "vendor": "Espressif",
  "url": "https://docs.espressif.com/projects/esp-idf/en/latest/esp32s3/hw-reference/esp32s3/user-guide-devkitc-1.html",
  "build": {
    "core": "esp32",
    "mcu": "esp32s3",
    "variant": "esp32s3",
    "f_cpu": "240000000L",
    "f_flash": "80000000L",
    "flash_mode": "qio",
    "extra_flags": [
      "-DARDUINO_ESP32S3_DEV",
      "-DBOARD_HAS_PSRAM",
      "-DARDUINO_USB_MODE=1",
      "-DARDUINO_USB_CDC_ON_BOOT=1"
    ],
    "arduino": {
      "memory_type": "qio_opi",
      "partitions": "default_16MB.csv",
      "ldscript": "esp32s3_out.ld"
    }
  },
  "connectivity": [
    "wifi",
    "bluetooth"
  ],
  "frameworks": [
    "arduino",
    "espidf"
  ],
  "upload": {
    "flash_size": "16MB",
    "maximum_size": 16777216,
    "maximum_ram_size": 327680,
    "speed": 921600,
    "require_upload_port": true
  },
  "debug": {
    "default_tool": "esp-builtin",
    "onboard_tools": [
      "esp-builtin"
    ],
    "openocd_target": "esp32s3.cfg"
  }
}
